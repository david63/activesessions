{
	"name": "david63/activesessions",
	"type": "phpbb-extension",
	"description": "Adds an Admin option to display the current active user sessions.",
	"version": "3.1.1",
	"time": "2020-01-17",
	"keywords": [
		"phpbb",
		"extension",
		"session",
		"ACP"
	],
	"license": "GPL-2.0-only",
	"authors": [{
		"name": "David Wood",
		"role": "Developer"
	}],
	"require": {
        "php": ">=7.1.3",
        "composer/installers": "~1.0"
    },
	"extra": {
		"display-name": "Active sessions",
		"soft-require": {
			"phpbb/phpbb": ">=3.3.0,<4.0.0@dev"
      	},
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/versions",
        	"filename": "active_sessions_version_file.json",
			"ssl": false
    	}
	}
}
